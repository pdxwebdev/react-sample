{
  "name": "matt-vogel",
  "version": "0.0.6",
  "private": "true",
  "description": "The creative works of Matt Vogel.",
  "scripts": {
    "prepare": "rm -rf dist/ && mkdir -p dist && mkdir -p dist/media",
    "copy": "npm run copy:index && npm run copy:media",
    "copy:index": "cp screens/index.html dist/index.html",
    "copy:media": "cp -R media/* dist/media/",
    "build:gh-pages": "npm run prepare && npm run copy && cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.gh-pages.js --bail",
    "check": "check-dependencies",
    "start": "npm run check && npm run prepare && npm run copy && webpack-dev-server --progress --colors --config webpack.config.dev.js",
    "lint": "eslint components screens theme",
    "test": "npm run lint",
    "gh-pages": "npm run build:gh-pages && gh-pages --dist dist && npm run prepare",
    "preversion": "npm test && npm run gh-pages",
    "postversion": "git push origin master && git push origin --tags",
    "category": "yo ./generators/category",
    "component": "yo ./generators/component"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pdxwebdev/react-sample.git"
  },
  "devDependencies": {
    "autoprefixer": "6.3.2",
    "babel-core": "6.4.5",
    "babel-eslint": "5.0.0-beta9",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.0.1",
    "babel-preset-stage-1": "6.3.13",
    "check-dependencies": "0.12.0",
    "component-playground": "1.2.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "docgen-loader": "1.3.3",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "4.0.0",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-react": "3.16.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gh-pages": "0.9.0",
    "img-loader": "1.2.2",
    "json-loader": "0.5.4",
    "jsontosass-loader": "0.1.8",
    "node-sass": "3.4.2",
    "postcss-loader": "0.8.0",
    "raw-loader": "0.5.1",
    "react-docgen": "2.7.0",
    "react-router": "2.0.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "svg-sprite-loader": "0.0.16",
    "url-loader": "0.5.7",
    "webpack": "1.12.12",
    "webpack-dev-middleware": "1.5.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.6.4",
    "yeoman-generator": "0.22.5",
    "yo": "1.6.0"
  },
  "dependencies": {
    "classnames": "2.2.3",
    "react": "0.14.7",
    "react-dom": "0.14.7"
  }
}
